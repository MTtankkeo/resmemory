  "description": "template for microservice ",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test jest   --forceExit ",
    "start:gate": "babel-node src/app.js",
    "start:dis": " babel-node src/modules/distributor.js",
    "start:users": "babel-node src/modules/users/users.module.js",
    "start:posts": " babel-node src/modules/posts/posts.module.js",
    "start:admin": "babel-node src/modules/admin/admin.module.js",
    "start:threads": " babel-node src/modules/threads/threads.module.js",
    "start:chat": " babel-node src/modules/chat/chat.module.js",
    "start:gate_build": "babel-node dist/app.js",
    "start:dis_build": "babel-node dist/modules/distributor.js",
    "start:users_build": "babel-node dist/modules/users/users.module.js",
    "start:posts_build": "babel-node dist/modules/posts/posts.module.js",
    "start:admin_build": "babel-node dist/modules/admin/admin.module.js",
    "start:threads_build": "babel-node dist/modules/threads/threads.module.js",
    "start:chat_build": "babel-node dist/modules/chat/chat.module.js",
    "start": "concurrently \"npm run start:gate\" \"npm run start:dis\" \"npm run start:users\" \"npm run start:posts\"  \"npm run start:admin\"  \"npm run start:threads\"  \"npm run start:chat\"",
    "build": " babel src --out-dir dist --copy-files "
  },
